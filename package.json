    "@icon-park/vue-next": "^1.4.2",
    "@vueuse/core": "^10.4.1",
    "@vueuse/integrations": "^10.5.0",
    "axios": "^1.5.1",
    "easy-typer-js": "^2.1.0",
    "idb-keyval": "^6.2.1",
    "pinia": "^2.1.6",
