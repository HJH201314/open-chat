{
  "name": "open-chat",
  "version": "1.0.0",
  "description": "An OpenAI chatbot interface",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:client\" \"pnpm run dev:server\"",
    "dev:client": "cd client && pnpm run dev",
    "dev:server": "cd server && pnpm run start:dev",
    "build": "ts-node build.ts",
    "build:client": "cd client && pnpm run build",
    "build:server": "cd server && pnpm run build",
    "start": "node dist/server/main.js"
  },
  "keywords": [
    "openai",
    "chatbot",
    "vue",
    "vite",
    "node",
    "nest.js"
  ],
  "author": "HJH201314",
  "license": "GPL-3.0",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "requirements": {
    "node": ">=16.7.0"
  }
}
