{
  "name": "open-chat",
  "version": "1.0.0",
  "description": "An OpenAI chatbot interface",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:client\" \"pnpm run dev:server\"",
    "dev:client": "cd client && pnpm run dev",
    "dev:server": "cd server && pnpm run start:dev",
    "build": "ts-node scripts/build.ts",
    "build:client": "cd client && pnpm run build",
    "build:server": "cd server && go build",
    "start": "node dist/server/main.js"
  },
  "keywords": [
    "openai",
    "chatbot",
    "vue",
    "vite",
    "node",
    "go"
  ],
  "author": "HJH201314",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/node": "^22.13.4",
    "concurrently": "^8.2.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.7.3"
  },
  "requirements": {
    "node": ">=20.0.0"
  }
}
