{
  "name": "open-chat-client",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.0.0+",
  "type": "module",
  "scripts": {
    "start": "concurrently \"pnpm run dev\" \"pnpm run serve\"",
    "dev": "vite --host",
    "dev:tauri": "pnpm tauri dev",
    "serve": "cd server && pnpm run dev",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview --host",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@icon-park/vue-next": "^1.4.2",
    "@liuli-util/tree": "^3.9.0",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@tsparticles/preset-confetti": "^3.2.0",
    "@tsparticles/slim": "^3.8.1",
    "@tsparticles/vue3": "^3.0.1",
    "@types/tinycolor2": "^1.4.6",
    "@vueuse/components": "^12.8.2",
    "@vueuse/core": "^12.8.2",
    "@vueuse/integrations": "^12.8.2",
    "@vueuse/router": "^12.8.2",
    "@vueuse/rxjs": "^12.8.2",
    "axios": "1.7.9",
    "dexie": "^4.0.11",
    "easy-typer-js": "^2.1.0",
    "highlight.js": "^11.11.1",
    "idb-keyval": "^6.2.1",
    "markdown-it": "^14.1.0",
    "markdown-it-highlightjs": "^4.2.0",
    "markdown-it-katex": "^2.0.3",
    "pinia": "^3.0.1",
    "rxjs": "^7.8.2",
    "sass": "^1.86.0",
    "tdesign-icons-vue-next": "^0.3.5",
    "tdesign-vue-next": "^1.11.5",
    "tinycolor2": "^1.6.0",
    "voice-input-button2": "^1.1.9",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.0",
    "@rushstack/eslint-patch": "^1.11.0",
    "@tauri-apps/cli": "^2.4.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/jsdom": "^21.1.7",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.13.11",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^15.15.0",
    "jsdom": "^26.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "swagger-typescript-api": "^13.0.24",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "unplugin-vue-components": "^28.4.1",
    "vite": "6.1.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-mkcert": "^1.17.8",
    "vite-plugin-pwa": "^0.21.2",
    "vite-plugin-sass-dts": "^1.3.31",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.9",
    "vue-tsc": "^2.2.8"
  }
}
