{
  "name": "open-chat-client",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.0.0+",
  "type": "module",
  "scripts": {
    "start": "concurrently \"pnpm run dev\" \"pnpm run serve\"",
    "dev": "vite --host",
    "serve": "cd server && pnpm run dev",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@icon-park/vue-next": "^1.4.2",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@vitejs/plugin-basic-ssl": "^1.2.0",
    "@vueuse/components": "^12.7.0",
    "@vueuse/core": "^12.7.0",
    "@vueuse/integrations": "^12.7.0",
    "@vueuse/router": "^12.7.0",
    "axios": "1.7.9",
    "easy-typer-js": "^2.1.0",
    "highlight.js": "^11.11.1",
    "idb-keyval": "^6.2.1",
    "markdown-it": "^14.1.0",
    "markdown-it-highlightjs": "^4.2.0",
    "markdown-it-katex": "^2.0.3",
    "pinia": "^3.0.1",
    "sass": "^1.85.0",
    "voice-input-button2": "^1.1.9",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.0",
    "@rushstack/eslint-patch": "^1.10.5",
    "@tsconfig/node18": "^18.2.4",
    "@types/jsdom": "^21.1.7",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.13.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.15.0",
    "jsdom": "^26.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.29",
    "prettier": "^3.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.24.0",
    "unplugin-vue-components": "^28.1.0",
    "vite": "6.1.0",
    "vite-plugin-pwa": "^0.21.1",
    "vite-plugin-sass-dts": "^1.3.30",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.2.2"
  }
}
